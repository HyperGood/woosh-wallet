{
  "expo": {
    "name": "Woosh MVP",
    "slug": "woosh-mvp-mobile",
    "version": "1.0.0",
    "scheme": "woosh-mvp-mobile",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "sentry-expo",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow Woosh to use Face ID"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos so you can select a profile picture."
        }
      ],
      "@react-native-firebase/app",
      "expo-build-properties",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow Woosh to access your contacts so you can send funds to anyone in your contacts list."
        }
      ],
      "expo-localization",
      "sentry-expo"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "buildNumber": "1",
      "bundleIdentifier": "com.hypergood.wooshmvpmobile",
      "usesAppleSignIn": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow Woosh to use Face ID",
        "NSContactsUsageDescription": "Allow Woosh to access your contacts so you can send funds to anyone in your contacts list."
      },
      "runtimeVersion": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ],
      "package": "com.hypergood.wooshmvpmobile",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0115e554-889d-4722-9140-d898c364853e"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "sentry org slug, or use the `SENTRY_ORG` environment variable",
            "project": "sentry project name, or use the `SENTRY_PROJECT` environment variable"
          }
        }
      ]
    },
    "updates": {
      "url": "https://u.expo.dev/0115e554-889d-4722-9140-d898c364853e"
    }
  }
}
