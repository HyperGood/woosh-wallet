{
  "expo": {
    "name": "woosh-mvp-mobile",
    "slug": "woosh-mvp-mobile",
    "version": "1.0.0",
    "scheme": "woosh-mvp-mobile",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      ["expo-local-authentication", { "faceIDPermission": "Allow Woosh to use Face ID" }]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.hypergood.wooshmvpmobile",
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0115e554-889d-4722-9140-d898c364853e"
      }
    }
  }
}
